ef="#o2034"/></CL_FP_INTERFACE_DATA_CHANGED><CL_FP_CODING classVersion="1"><INPUT_PARAMETERS><FPPARAMETER>GT_SHP_LABEL</FPPARAMETER><FPPARAMETER>LS_ETIQUETA</FPPARAMETER><FPPARAMETER>HU_HEADER</FPPARAMETER><FPPARAMETER>SHP_LABEL</FPPARAMETER></INPUT_PARAMETERS><OUTPUT_PARAMETERS><FPPARAMETER>GT_SHP_LABEL</FPPARAMETER><FPPARAMETER>GV_HUIDENT</FPPARAMETER><FPPARAMETER>LS_ETIQUETA</FPPARAMETER></OUTPUT_PARAMETERS><INITIALIZATION><FPCLINE>*BREAK mqa_abap2.</FPCLINE><FPCLINE/><FPCLINE>gt_shp_label[] = shp_label.</FPCLINE><FPCLINE/><FPCLINE>READ TABLE hu_header INTO DATA(ls_header) INDEX 1.</FPCLINE><FPCLINE>IF sy-subrc EQ 0.</FPCLINE><FPCLINE/><FPCLINE>  SELECT SINGLE * FROM /scwm/tmapwhnum INTO @DATA(ls_tmapwhnum) WHERE whnumwme EQ @ls_header-lgnum.</FPCLINE><FPCLINE/><FPCLINE>  SELECT SINGLE * FROM t320 INTO @DATA(ls_t320) WHERE lgnum EQ @ls_tmapwhnum-whnumerp(3).</FPCLINE><FPCLINE/><FPCLINE>  SELECT SINGLE * FROM t001w INTO @DATA(ls_t001w) WHERE werks EQ @ls_t320-werks.</FPCLINE><FPCLINE/><FPCLINE>  ls_etiqueta-address = |{ ls_t001w-stras },{ ls_t001w-cityc }|.</FPCLINE><FPCLINE>  ls_etiqueta-created_by = ls_header-created_by.</FPCLINE><FPCLINE>  ls_etiqueta-hu_ident = gv_huident = |{ ls_header-huident ALPHA = OUT }|.</FPCLINE><FPCLINE/><FPCLINE>  SELECT SINGLE lnumt FROM /scwm/t300t INTO ls_etiqueta-source WHERE spras EQ 'EN' AND lgnum EQ ls_header-lgnum.</FPCLINE><FPCLINE/><FPCLINE>ENDIF.</FPCLINE><FPCLINE/><FPCLINE>LOOP AT material_items INTO DATA(ls_items).</FPCLINE><FPCLINE>  ls_etiqueta-quanty += ls_items-quan.</FPCLINE><FPCLINE>  ls_etiqueta-unit   = ls_items-meins.</FPCLINE><FPCLINE>ENDLOOP.</FPCLINE><FPCLINE/><FPCLINE>CONDENSE: ls_etiqueta-quanty, ls_etiqueta-unit.</FPCLINE><FPCLINE>ls_etiqueta-quanty = |{ ls_etiqueta-quanty } { ls_etiqueta-unit }|.</FPCLINE><FPCLINE/><FPCLINE/><FPCLINE>DELETE gt_shp_label WHERE erpno EQ ''.</FPCLINE><FPCLINE/><FPCLINE>LOOP AT shp_label INTO DATA(ls_ship).</FPCLINE><FPCLINE>* SHP_LABEL-ERPNO: Reference document</FPCLINE><FPCLINE>  IF ls_ship-erpno IS NOT INITIAL.</FPCLINE><FPCLINE>    SELECT SINGLE * FROM lips INTO @DATA(ls_lips) WHERE vbeln EQ @ls_ship-erpno.</FPCLINE><FPCLINE>  ENDIF.</FPCLINE><FPCLINE/><FPCLINE>  IF ls_ship-addrno IS NOT INITIAL.</FPCLINE><FPCLINE>    SELECT SINGLE * FROM adrc INTO @DATA(ls_adrc) WHERE addrnumber EQ @ls_ship-addrno.</FPCLINE><FPCLINE>  ENDIF.</FPCLINE><FPCLINE/><FPCLINE>  IF ls_ship-custno IS NOT INITIAL.</FPCLINE><FPCLINE>    ls_etiqueta-custno = ls_ship-custno.</FPCLINE><FPCLINE/><FPCLINE>  ENDIF.</FPCLINE><FPCLINE>ENDLOOP.</FPCLINE><FPCLINE/><FPCLINE/><FPCLINE>ls_etiqueta-order = |{ ls_lips-vgbel ALPHA = OUT }|.</FPCLINE><FPCLINE/><FPCLINE>ls_etiqueta-docno = |{ ls_ship-docno ALPHA = OUT }|.</FPCLINE><FPCLINE/><FPCLINE>ls_etiqueta-hu_date = sy-datum.</FPCLINE><FPCLINE/><FPCLINE>*SHIFT ls_etiqueta-hu_ident LEFT DELETING LEADING '0'.</FPCLINE><FPCLINE/><FPCLINE>*CONDENSE ls_etiqueta.</FPCLINE><FPCLINE>*Recept. Address: SHP_LABEL-ADDRESS (SHP_LABEL-COO)</FPCLINE><FPCLINE>*ODO: SHP_LABEL-DOCNO</FPCLINE><FPCLINE>*Out.Del: SHP_LABEL-CUSTNO</FPCLINE><FPCLINE>*Address: SHIP_LABEL-ADDRESS (SHP_LABEL-ADDRNO)</FPCLINE></INITIALIZATION><FORMS/></CL_FP_CODING></cls:CL_FP_CODING></asx:heap></asx:abap>